<link rel="stylesheet" href="/public/style.css">
<div id="div">
    <input type="text" id="input" size=20 placeholder="Search a composer .."/>
    <button id="button">fetch</button>
    <br>
    <ul>
    <% if (typeof(composerIds) !== 'undefined') {
        for (const [composer, id] of Object.entries(composerIds)) { %>
            <li><a href="work_composer=<%=id%>"><%=composer%></a></li>
        <% } %>
    <% } %>
    </ul>
</div>
<script src="/public/scripti.js"></script>
